{
  "name": "webdevetc/blogetc",
  "keywords": [
    "laravel",
    "blog",
    "package",
    "admin",
    "panel",
    "rss",
    "feed",
    "write",
    "posts",
    "news",
    "update",
    "webdevetc",
    "webdev"
  ],
  "description": "Simple blog (with admin panel) for Laravel from https://webdevetc.com/",
  "license": "MIT",
  "support": {
    "docs": "https://webdevetc.com/laravel/packages/blogetc-blog-system-for-your-laravel-app/help-documentation/laravel-blog-package-blogetc"
  },
  "authors": [
    {
      "name": "WebDevEtc",
      "homepage": "https://webdevetc.com/",
      "role": "developer",
      "email": "packages@webdevetc.com"
    }
  ],
  "require": {
    "intervention/image": "2.*",
    "cviebrock/eloquent-sluggable": "^11.0",
    "laravel/framework": "^9.0|^10.0|^11.0|^12.0",
    "laravel/helpers": "^1.2",
    "rumenx/php-feed": "^1.0@beta"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "orchestra/testbench": "^6.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "WebDevEtc\\BlogEtc\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebDevEtc\\BlogEtc\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "WebDevEtc\\BlogEtc\\BlogEtcServiceProvider"
      ],
      "aliases": {
      }
    }
  },
  "abandoned": true
}
